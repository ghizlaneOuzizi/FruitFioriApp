<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns="sap.m" controllerName="algo.fiori.controller.View2">
<Page title="View 2" showNavButton="true" navButtonPress="onGoBack">
 <content>
   <ObjectHeader title="{name}" intro="{taste}" number="{price}" numberUnit="{currency}" icon="{image}">
   </ObjectHeader>
   <IconTabBar>
     <items>
       <IconTabFilter text="More Info" icon="sap-icon://warning">
          <core:Fragment fragmentName="algo.fiori.fragment.moreInfo" type="XML"/>
       </IconTabFilter>
       <IconTabFilter text="Suppliers" icon="sap-icon://supplier">
          <core:Fragment fragmentName="algo.fiori.fragment.suppliers" type="XML"/>
       </IconTabFilter>
       <IconTabFilter text="Cities" icon="sap-icon://home">
          <core:Fragment fragmentName="algo.fiori.fragment.cities" type="XML"/>
       </IconTabFilter>
     </items>
   </IconTabBar>
  <!-- <Button text="Go Back" press="onGoBack"></Button> -->
 </content>
 <footer>
   <Toolbar >
      <ToolbarSpacer ></ToolbarSpacer>
      <Button text="Save" type="Accept" icon="sap-icon://save" press="onSave"></Button>
      <Button text="Cancel" icon="sap-icon://cancel" press="onCancel"></Button>
   </Toolbar>
 </footer>
</Page>
</mvc:View>