<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns:viz="sap.viz.ui5.controls" 
xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"
xmlns="sap.m" xmlns:viz.data="sap.viz.ui5.data" controllerName="algo.fiori.controller.View3">
<Page title="View 3" showNavButton="true" navButtonPress="onGoBack">
  <content>
   <ObjectHeader title="{name}" intro="{city}" number="{numberOfStores}"
    numberUnit="{personName} {contact}"></ObjectHeader>
     <Select id="chartType" selectedKey="{/chartType}">
       <items>
        <core:Item text="Column" key="column"></core:Item>
        <core:Item text="Donut" key="donut"></core:Item>
        <core:Item text="Line" key="line"></core:Item>
       </items>
     </Select>

     <viz:VizFrame id="idVizFrame" uiConfig="{applicationSet:'fiori'}"
                height='60%' width="60%" vizType='{/chartType}'>
                <viz:dataset>
                    <viz.data:FlattenedDataset data="{specialties}">
                        <viz.data:dimensions>
                            <viz.data:DimensionDefinition name="Name"
                                value="{name}" />
                        </viz.data:dimensions>
                        <viz.data:measures>
                            <viz.data:MeasureDefinition name="Quatity"
                                value="{supply}" />
                        </viz.data:measures>
                    </viz.data:FlattenedDataset>
                </viz:dataset>

                <viz:feeds>
                    <viz.feeds:FeedItem id='valueAxisFeed' uid="valueAxis" type="Measure"
                        values="Quatity" />
                    <viz.feeds:FeedItem id='categoryAxisFeed' uid="categoryAxis" type="Dimension"
                        values="Name" />
                </viz:feeds>
                <viz:dependents>
                    <viz:Popover id="idPopOver"></viz:Popover>
                </viz:dependents>
            </viz:VizFrame>
  </content>
</Page>
</mvc:View>